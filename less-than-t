/* A program written by Weronika Wolska
	to input a value for T and give back the 
	number of values in the array that are
	less than T */
#include <stdio.h>

int main(void)
{
	int T;
	int a[20];
	int i;
	int count = 0;
	
	printf("Enter the value of T: \t");
	scanf("%d", &T);
	
	printf("Enter values into the array: \n");
	
	for(i=0; i<10; i++)
	{
		scanf("%d \n", &a[i]);
	}
	
	for(i=0;i<10;i++)
	{
		if(a[i]<T)
			count++;
	}
	
	printf("%d", count);
	
	return 0;
}
	
